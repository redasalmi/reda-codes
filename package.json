		"url": "https://github.com/redasalmi/reda-codes/issues"
	},
	"homepage": "https://github.com/redasalmi/reda-codes#readme",
	"scripts": {
		"predev": "rimraf ./public/_redirects",
		"dev": "remix dev",
		"start": "cross-env NODE_ENV=production netlify dev",
		"build": "remix build",
		"lint": "eslint --ext js,jsx,ts,tsx .",
		"typecheck": "tsc -b",
		"validate": "npm-run-all -p lint typecheck test:ci build",
		"clean": "rimraf ./node_modules/.cache ./public/build ./netlify ./build",
		"prepare": "husky install",
		"test": "vitest",
		"test:ci": "vitest run"
	},
	"lint-staged": {
		"**/*.+(js|jsx|ts|tsx|json|css)": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"@netlify/functions": "^1.6.0",
		"@remix-run/css-bundle": "^1.18.1",
		"@remix-run/netlify": "^1.18.1",
		"@remix-run/node": "^1.18.1",
		"@remix-run/react": "^1.18.1",
		"framer-motion": "^10.12.18",
		"isbot": "^3.6.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.18.1",
		"@remix-run/eslint-config": "^1.18.1",
		"@remix-run/serve": "^1.18.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@vitejs/plugin-react": "^4.0.1",
		"autoprefixer": "^10.4.14",
		"cross-env": "^7.0.3",
		"cssnano": "^6.0.1",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"happy-dom": "^9.20.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.24",
		"postcss-cli": "^10.1.0",
