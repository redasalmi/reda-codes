{
  "private": true,
  "name": "redasalmi-portfolio-v2",
  "version": "1.0.0",
  "description": "Reda Salmi Portfolio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redasalmi/redasalmi-portfolio-v2.git"
  },
  "author": "Reda Salmi <reda.salmi.elt@gmail.com> (https://redasalmi.netlify.app/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redasalmi/redasalmi-portfolio-v2/issues"
  },
  "homepage": "https://github.com/redasalmi/redasalmi-portfolio-v2#readme",
  "scripts": {
    "css:build": "postcss styles/*.css --dir app/styles",
    "css:build:watch": "npm run css:build -- --watch",
    "css:build:prod": "npm run css:build -- --env production",
    "svg:build": "npx @svgr/cli -- svg",
    "svg:build:watch": "nodemon --watch svg --verbose -e svg --exec \"npm run svg:build\"",
    "dev": "concurrently \"npm run css:build:watch\" \"npm run svg:build:watch\" \"npm run dev:netlify\"",
    "dev:netlify": "cross-env NODE_ENV=development BROWSER=none netlify dev",
    "start": "npm run build && cross-env NODE_ENV=production BROWSER=none netlify dev",
    "prebuild": "npm run clean",
    "build": "npm run css:build:prod && npm run svg:build && remix build",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "typecheck": "npm run svg:build && tsc -b",
    "validate": "concurrently --kill-others-on-fail --prefix [{name}] --names lint,typecheck,build \"npm run lint --silent\" \"npm run typecheck --silent\" \"npm run build --silent\"",
    "clean": "rimraf ./node_modules/.cache ./app/styles ./app/components/icons ./public/build ./netlify",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx|json|css)": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.3.3",
    "@remix-run/node": "^1.3.3",
    "@remix-run/react": "^1.3.3",
    "framer-motion": "^6.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@svgr/cli": "^6.2.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "sideEffects": false
}
