{
  "private": true,
  "name": "redasalmi-portfolio-v2",
  "description": "Reda Salmi Portfolio",
  "license": "MIT",
  "scripts": {
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development netlify dev",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "validate": "concurrently --kill-others-on-fail --prefix [{name}] --names lint,build \"npm run lint --silent\" \"npm run build --silent\"",
    "postinstall": "remix setup node",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx|json|css)": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "concurrently": "^6.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^6.0.0",
    "lint-staged": "^12.1.4",
    "prettier": "2.5.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "sideEffects": false
}
